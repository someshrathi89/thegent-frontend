{
  "expo": {
    "name": "TheGent",
    "slug": "thegent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "thegent",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.thegent.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "TheGent needs camera access to capture your photos for style analysis",
        "NSPhotoLibraryUsageDescription": "TheGent needs photo library access to select images for style analysis",
        "NSPhotoLibraryAddUsageDescription": "TheGent needs permission to save your style results to your photo library",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.thegent.app",
      "versionCode": 10,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#1a1a2e"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "TheGent needs camera access to capture your photos for style analysis"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "TheGent needs photo library access to select images for style analysis"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "734f20c2-3b8e-4f71-9e4d-05b79e88d003"
      }
    },
    "owner": "someshrathi89"
  }
}
